{
    "name": "arthurkushman/php-wss",
    "description": "Web-socket server with URI parse and multi-process and PROXY support",
    "keywords": [
        "web-sockets",
        "web socket server",
        "web socket client",
        "web socket",
        "processes",
        "routes",
        "php"
    ],
    "license": "MIT",
    "homepage": "https://github.com/arthurkushman/php-wss",
    "require": {
        "php": ">=7.1",
        "ext-pcntl": "*"
    },
    "autoload": {
        "psr-4": {
            "WSSC\\": "src",
            "WSSCTEST\\": "tests"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WSSC\\": "src",
            "WSSCTEST\\": "tests"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^7",
        "monolog/monolog": "^1.24"
    }
}
